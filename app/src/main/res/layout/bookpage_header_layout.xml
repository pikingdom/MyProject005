<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <com.nd.hilauncherdev.plugin.navigation.widget.bookpage.header.item.BookPageTitleLayout
        android:id = "@+id/layout_bookpage_title_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.nd.hilauncherdev.plugin.navigation.widget.bookpage.header.item.BookPageTitleLayout>


    <com.nd.hilauncherdev.plugin.navigation.widget.bookpage.header.item.BookPageShelfLayout
        android:id="@+id/view_bookpage_shelf"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"></com.nd.hilauncherdev.plugin.navigation.widget.bookpage.header.item.BookPageShelfLayout>

    <com.nd.hilauncherdev.plugin.navigation.widget.bookpage.header.item.BookPageEntranceLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.nd.hilauncherdev.plugin.navigation.widget.bookpage.header.item.BookPageEntranceLayout>

</LinearLayout>